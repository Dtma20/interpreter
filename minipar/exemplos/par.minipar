func fat(n: num) -> array {
	f: array[n + 1]
	f[0] = 1
	f[1] = 1

	for(i: num = 2; i <= n; i++) {
		f[i] = f[i - 1] * i
	}
	return f
}

func fib(n: num) -> array {
	f: array[n + 1]
	f[0] = 0
	f[1] = 1
	for(i: num = 2; i <= n; i++) {
		f[i] = f[i - 1] + f[i - 2]
	}
	return f
}

n: num = to_num(input("Digite um nÃºmero: "))

arr1: array[n + 1] = fib(n)
arr2: array[n + 1] = fat(n)
par {
	print(arr1, "\n")
	print(arr2, "\n")
}